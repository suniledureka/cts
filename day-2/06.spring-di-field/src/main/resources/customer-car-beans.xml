<?xml version="1.0" encoding="UTF-8"?>

<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns:context="http://www.springframework.org/schema/context"
	xsi:schemaLocation="http://www.springframework.org/schema/beans 
						http://www.springframework.org/schema/beans/spring-beans.xsd
						http://www.springframework.org/schema/context 
						http://www.springframework.org/schema/context/spring-context-4.3.xsd">
    
    <context:annotation-config />
    
    <bean id="bmw" class="com.cognizant.spring.core.beans.Car">
    	<property name="model" value="B M W"/>
    	<property name="cost" value="7500000.50"/>
    </bean>    
    
    <bean id="sanjay" class="com.cognizant.spring.core.beans.Customer">
    	<property name="customerName" value="Sanjay Patil"/>    	
    </bean>  
    
    <bean id="audi" class="com.cognizant.spring.core.beans.Car" primary="true">
    	<property name="model" value="Audi"/>
    	<property name="cost" value="9300000"/>
    </bean>        
</beans>        